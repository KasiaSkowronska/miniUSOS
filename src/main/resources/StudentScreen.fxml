<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniUSOS.StudentScreenController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button layoutX="410.0" layoutY="69.0" mnemonicParsing="false" onAction="#switchToGroup" text="Grupy" />
            <Button layoutX="316.0" layoutY="69.0" mnemonicParsing="false" onAction="#switchToCourse" text="Katalog" />
            <Label layoutX="27.0" layoutY="25.0" text="Okno studenta" />
            <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="27.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true" />
            <Button layoutX="438.0" layoutY="328.0" mnemonicParsing="false" onAction="#createStudent" text="Dodaj studenta" />
            <Label fx:id="nameField" layoutX="137.0" layoutY="215.0" text="Label" />
            <Label layoutX="33.0" layoutY="214.0" text="Zalogowano:" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
